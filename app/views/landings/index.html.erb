<div class="container">
  <div class="row">
    <% if current_user %>
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class='panel-title'>
          Dashboard
        </h3>
      </div>
      <div class="panel-body">
        
      </div>
    </div>
    <% else %>
      <div class='jumbotron'>
        <h1>Welcome to ShiftSlot</h1>
        <p>
          ShiftSlot is a place for volunteers and workers to sign up for shifts at some Seattle music venues. <%= link_to 'Sign in', user_session_path %> or <%= link_to 'Sign up', new_user_registration_path %> above to get started.
        </p>
      </div>
    <% end %>
  </div>
</div>
